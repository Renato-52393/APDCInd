package pt.unl.fct.di.apdc.projindividual.util;
public class RegisterData {

	private static final String US = "US";
	//required
	public String username;
	public String email;
	public String pwd;
	public String pwd_conf;
	public String role;
	
	//optional
	public String profile;
	public int telephone;
	public int mobile;
	public String adress;
	public String comp_adress;
	public String local;

	public RegisterData() {}

	public RegisterData(String username, String email, String pwd, String pwd_conf, String profile, int telephone, int mobile, String adress, String comp_adress, String local) {
		this.username = username;
		this.pwd = pwd;
		this.pwd_conf = pwd_conf;
		this.email = email;
		this.profile = profile;
		this.telephone = telephone;
		this.mobile = mobile;
		this.adress = adress;
		this.comp_adress = comp_adress;
		this.local = local;
	}
	
}
